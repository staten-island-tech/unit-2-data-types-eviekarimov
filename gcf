def Greatest_Common_Factor(m,n):
    if n == 0:
        return m
    else:
        return Greatest_Common_Factor(n, m % n)
m = int(input("Give me your number:"))
n = int(input("Give me your number:"))

print("The Greatest Common Factor is:", Greatest_Common_Factor(m,n))

